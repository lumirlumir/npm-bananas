{
  "name": "bananass-utils-console",
  "version": "0.0.0",
  "type": "module",
  "description": "Console Utilities for Bananass Framework.üçå",
  "exports": {
    "./logger": {
      "types": "./build/logger.d.ts",
      "default": "./src/logger.js"
    },
    "./spinner": {
      "types": "./build/spinner.d.ts",
      "default": "./src/spinner.js"
    },
    "./theme": {
      "types": "./build/theme.d.ts",
      "default": "./src/theme.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./build/*"
      ],
      "logger": [
        "./build/logger.d.ts"
      ],
      "spinner": [
        "./build/spinner.d.ts"
      ],
      "theme": [
        "./build/theme.d.ts"
      ]
    }
  },
  "files": [
    "src",
    "build",
    "LICENSE.md",
    "README.md",
    "!src/**/*.test.js",
    "!**/fixtures/**"
  ],
  "keywords": [
    "bananass",
    "baekjoon",
    "framework",
    "javascript",
    "utils",
    "console",
    "icon",
    "logger",
    "spinner",
    "theme"
  ],
  "author": "Î£®Î∞ÄLuMir <rpfos@naver.com> (https://github.com/lumirlumir)",
  "license": "MIT",
  "homepage": "https://bananass.lumir.page",
  "repository": {
    "type": "github",
    "url": "git+https://github.com/lumirlumir/npm-bananass.git",
    "directory": "packages/bananass-utils-console"
  },
  "bugs": {
    "url": "https://github.com/lumirlumir/npm-bananass/issues"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "publishConfig": {
    "provenance": true
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npx tsc && shx cp ../../LICENSE.md ../../README.md .",
    "test": "node --test"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "is-interactive": "^2.0.0",
    "is-unicode-supported": "^2.1.0"
  },
  "devDependencies": {
    "get-stream": "^9.0.1",
    "strip-ansi": "^7.1.0"
  }
}
